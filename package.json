{
  "name": "ssp-ui",
  "description": "ReactJS component library with crisp, elegant and super-easy-to-use components.",
  "author": "Jiten Poddar",
  "private": true,
  "version": "0.3.8",
  "license": "MIT",
  "main": "ssp-ui.umd.js",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "build": "npm run build:es2015 && npm run build:es2015module && npm run build:umd",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --out-dir ./dist",
    "build:es2015module": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./dist/ssp-ui.esm.js",
    "build:umd": "webpack --config webpack.config.js",
    "build-docs": "webpack --config docs/webpack.docs.config.js",
    "start-docs": "webpack-dev-server --content-base build/ --config docs/webpack.docs.config.js --watch --port 8000 --progress --inline --hot --history-api-fallback",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://git.metrosystems.net/lakshaya.sood/ssp-ui.git"
  },
  "peerDependencies": {
    "material-ui": "^0.19.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-backgrounds": "^3.0.0",
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/addon-options": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.1.4",
    "babel-loader": "^6.1.0",
    "babel-plugin-transform-runtime": "^6.1.4",
    "babel-polyfill": "^6.1.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-app": "^0.2.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.1.4",
    "babel-runtime": "^6.0.14",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.8.5",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.0",
    "react": "^15.6.1",
    "react-data-grid": "^2.0.53",
    "react-dom": "^15.4.1",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^2.0.0",
    "react-widgets": "3.4.5",
    "storybook-addon-code": "^0.1.1",
    "storybook-addon-material-ui": "^0.8.2",
    "style-loader": "^0.13.0",
    "typeface-lato": "0.0.35",
    "url-loader": "^0.5.6",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.5.4",
    "material-ui": "^0.19.3"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "classnames": "^2.2.5",
    "formsy-react": "^0.19.0",
    "fs-extra": "^4.0.2",
    "lodash": "^4.16.2",
    "moment": "^2.17.1"
  }
}
